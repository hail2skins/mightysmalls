<%= simple_form_for([@owner, @business]) do |f| %>
<%= f.error_notification %>

	<div class="form-inputs">
		<%= f.input :name, label: "Business name", required: true %>
		<%= f.input :description %>
	</div>

	<div class="form-actions">
  	<%= f.submit "Create my account", class: 'btn btn-lg btn-primary' %>
  </div>
<% end %>